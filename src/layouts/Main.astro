---
import BaseHead from '../components/BaseHead.astro';
import Header from "@components/react/Header";
import Aside from "@components/react/Aside";
import Footer from "@components/react/Footer";

const { frontmatter} = Astro.props;
const { title, description, publishDate, updateDate, ogtype, ogimage, twimage } = typeof frontmatter !== "undefined" ? frontmatter : Astro.props;

const { pathname } = Astro.url;
---

<html lang="en">
	<BaseHead title={title}
						description={description}
						publishDate={publishDate}
						updateDate={updateDate}
						ogtype={ogtype}
						ogimage={ogimage}
						twimage={twimage}
						/>
  <body>
		<div class="wrapper">
      <Header path={pathname} />
      <div class="main-aside">
        <main>
					<!-- NOTE: Excluding H1 Header for homepage. -->
					{ pathname !== "/" &&
	          <h1>{title}</h1>
					}
          <slot />
        </main>
        <Aside />
      </div>
      <Footer />
    </div>
